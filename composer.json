{
    "name": "nikba/bussystem",
    "description": "A Laravel package for interacting with the Bus System API.",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Bargan Nicolai",
            "email": "office@nikba.com"
        }
    ],
    "require": {
        "php": ">=8.2"
    },
    "autoload": {
        "psr-4": {
            "nikba\\bussystem\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nikba\\BusSystem\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nikba\\BusSystem\\BusSystemServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
